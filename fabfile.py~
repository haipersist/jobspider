from fabric.api import *


class _Arrtributedict(dict):

    def __setattr__(self,key,value):

        self[key]=value

    def __getattr__(self,key):

        try:
            return self[key]
        except KeyError:
            raise AttributeError(key)


env.roledefs={'216':['hadoop@10.18.130.216',],'224':['tea@10.21.1.200',]}

@roles('216')
def exec_216():
    run('bash /home/hadoop/dumpdata.sh')
    local('scp 216:/home/hadoop/dumpdata.sh /cygdrive/e/work/')


@roles('tmp7')
def tmp7_del():
    run('rm /opt/tools/old_version/output_apk/* celeryd -l info >& /dev/null < /dev/null')


